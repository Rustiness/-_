<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="quest">
	<!--  -->
	<select id='cate' resultType="quest">
		SELECT questDF, name
		FROM DF_Question
		WHERE isYn = 'Y'
	</select>
</mapper>
